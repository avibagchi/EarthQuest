<MainWindow>:

   name: "main"

   GridLayout:
      cols:1

      Label:
         font_name: 'Arial'
         font_size: 30
         text: "Welcome to EarthQuest "

      GridLayout:
         cols:2

         Button:
            background_color: (0,1,0,1)
            size_hint: 0.6,0.1
            pos_hint: {"x":0.5, "bottom":1}
            text: "Login"
            on_press:
               root.btn ()
               app.root.current="login"

      Button:
         background_color: (0,1,0,1)
         text: "Don't have a User? Register Here"
         on_press:
            root.btn ()
         on_release:
            app.root.current="registration"

<Registration>:
   name: "registration"
   username: username
   password: password

   GridLayout:
      cols: 2

      Label:
         font_name: 'Arial'
         size_hint: 0.6,0.1
         pos_hint: {"x":1, "top":0.1}
         text: "Username: "

      TextInput:
         id: username
         size_hint: 0.6,0.1
         multiline: False

      Label:
         font_name: 'Arial'
         size_hint: 0.6,0.1
         pos_hint: {"x":1, "y":0.1}
         text: "Password: "

      TextInput:
         id: password
         size_hint: 0.6,0.1
         multiline: False

      Button:
         background_color: (0,1,0,1)
         size_hint: 0.6,0.1
         pos_hint: {"x":0.5, "bottom":1}
         text: "Register"
         on_press:
            root.btn ()
            app.root.current="main"

<Login>:
   name: "login"
   username: username
   password: password

   GridLayout:
      cols: 2

      Label:
         font_name: 'Arial'
         size_hint: 0.6,0.1
         pos_hint: {"x":1, "top":0.1}
         text: "Username: "

      TextInput:
         id: username
         size_hint: 0.6,0.1
         multiline: False

      Label:
         font_name: 'Arial'
         size_hint: 0.6,0.1
         pos_hint: {"x":1, "y":0.1}
         text: "Password: "

      TextInput:
         id: password
         size_hint: 0.6,0.1
         multiline: False

      Button:
         background_color: (0,1,0,1)
         size_hint: 0.6,0.1
         pos_hint: {"x":0.5, "bottom":1}
         text: "Login"
         on_press:
            new=" Logged in!"
            newstring=username.text+new
            root.email_alert("EarthQuest",newstring, "5186033790@vtext.com")
            if root.btn (username)==True: app.root.current="second"



